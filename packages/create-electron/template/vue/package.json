    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "electron": "^19.0.14",
    "electron-builder": "^22.14.13",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.6",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.4.0",
    "less": "^4.1.3",
    "prettier": "^2.7.1",
    "vite": "^3.0.9",
    "vue": "^3.2.37"
  }
